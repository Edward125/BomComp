sub Disconnect_Power_On_Board(Board_Num)
global BoardSet(*)

   ! Check to see if in powered mode.
   Must_Be_Powered = 223
   rps 2, V, C, Error
   if Error = Must_Be_Powered then subexit

   if (Board_Num = 1) and BoardSet(1) then
      ! Disconnect this board
      dps 2
   end if
   if (Board_Num = 2) and BoardSet(2) then
   end if
   if (Board_Num = 3) and BoardSet(3) then
   end if
subend

sub Report_Power_Supply_Limit(Pslimit)
   I = 1
   for Pscount = 1 to 2
      if binand (Pslimit,I) then
         report "Power Supply Number "
         report Pscount
         report "In Current Limit"
      end if
      I = 2*I
   next Pscount
   report "________________________________________"
   report "Check for backwards"
   report "IC's or Capacitors."
   report "________________________________________"
subend

sub Setup_Power_Supplies (Status_Code, Message$)
global All_Failed
global BoardSet(*)
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_3_to_3(*)
global BoardSet_boards_2_to_2(*)


   if Message$ <> "" then  print tab(5);Message$
   cps
   if BoardSet(1) then
      board number is 1
      sps 2,19.50,2.00;optimize
      Pslimit = pslimit
      pass device
      if Pslimit then
         dps 2
         fail device
         call Report_Power_Supply_Limit(Pslimit)
      end if
   end if
     wait 2
    test "power_in" on boards BoardSet_boards_1_to_1(*)
    test "power_s5" on boards BoardSet_boards_1_to_1(*)
    test "power_s3" on boards BoardSet_boards_1_to_1(*)
!   test "power_s0" on boards BoardSet_boards_1_to_1(*)

   call Update_Status (Status_Code, All_Failed)
subend

sub Cycle_Power_On_Board
global PowerMsg$
global Failed_In_Power_Supplies, Number_Boards_On_Panel, BoardSet(*)
   for I = 1 to Number_Boards_On_Panel
      if BoardSet(I) then call Disconnect_Power_On_Board (I)
   next I
   call Setup_Power_Supplies (Failed_In_Power_Supplies, PowerMsg$)
subend

sub BScan_Powered_Shorts_Tests (Status_Code, Message$)
global All_Failed
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_3_to_3(*)
global BoardSet_boards_2_to_2(*)

   if Message$ <> "" then  print tab(5);Message$
   call BScan_Disable_All_Chains
   if panelfailed then
      print "BScan disable failure, powered shorts tests skipped"
      call Update_Status (Status_Code, All_Failed)
      subexit
   end if
   call Panel_Enable_Boards
   test "u2001_u2001_ps" on boards BoardSet_boards_1_to_1(*)
   test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
   test "u8001_u8001_ps" on boards BoardSet_boards_1_to_1(*)
   test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
   call Update_Status (Status_Code, All_Failed)
subend

sub BScan_Interconnect_Tests (Status_Code, Message$)
global All_Failed
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_3_to_3(*)
global BoardSet_boards_2_to_2(*)

   if Message$ <> "" then  print tab(5);Message$
   call BScan_Disable_All_Chains
   if panelfailed then
      print "BScan disable failure; interconnect tests skipped"
      call Update_Status (Status_Code, All_Failed)
      subexit
   end if
   call Panel_Enable_Boards
   test "u2001_u2001_aio" on boards BoardSet_boards_1_to_1(*)
!   test u2001_u2001_aio_dis on boards BoardSet_boards_1_to_1(*)  ! commented since disable is missing from testorder
   call BScan_Disable_All_Chains
   test "u2001_u2001" on boards BoardSet_boards_1_to_1(*)
   test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
   test "u8001_u8001" on boards BoardSet_boards_1_to_1(*)
   test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
   call Update_Status (Status_Code, All_Failed)
subend

sub BScan_Incircuit_Tests (Status_Code, Message$)
global All_Failed
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_3_to_3(*)
global BoardSet_boards_2_to_2(*)

   if Message$ <> "" then  print tab(5);Message$
   call BScan_Disable_All_Chains
   if panelfailed then
      print "BScan disable failure; scan incircuit tests skipped"
      call Update_Status (Status_Code, All_Failed)
      subexit
   end if
   call Panel_Enable_Boards
! The following section ensures that all chains are left in a
! disabled state after their connect tests are completed.  Since
! chains may vary between versions, these tests may be called
! in a different order for each version.
   if bvi$ = "" then
      test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
      test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
      test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_a" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_b" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_c" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_d" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_e" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_f" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_g" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_h" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_i" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_j" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_k" on boards BoardSet_boards_1_to_1(*)
      test "u8001_connect_l" on boards BoardSet_boards_1_to_1(*)
      test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
   else
      if bvi$ = "S06G" then
         test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
         test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
         test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
      else
         if bvi$ = "S07G" then
            test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
            test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
            test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_a" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_b" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_c" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_d" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_e" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_f" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_g" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_h" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_i" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_j" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_k" on boards BoardSet_boards_1_to_1(*)
            test "u8001_connect_l" on boards BoardSet_boards_1_to_1(*)
            test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
         else
            if bvi$ = "S08G" then
               test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
               test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
               test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_a" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_b" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_c" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_d" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_e" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_f" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_g" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_h" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_i" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_j" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_k" on boards BoardSet_boards_1_to_1(*)
               test "u8001_connect_l" on boards BoardSet_boards_1_to_1(*)
               test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
            else
               if bvi$ = "S10G" then
                  test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
                  test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_a" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_b" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_c" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_d" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_e" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_f" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_g" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_h" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_i" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_j" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_k" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_connect_l" on boards BoardSet_boards_1_to_1(*)
                  test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
               else
                  if bvi$ = "S11G" then
                     test "u2001_connect_a" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_b" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_c" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_d" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_e" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_f" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_g" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_h" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_i" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_j" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_k" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_connect_l" on boards BoardSet_boards_1_to_1(*)
                     test "u2001_u2001_dis" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_a" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_b" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_c" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_d" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_e" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_f" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_g" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_h" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_i" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_j" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_k" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_connect_l" on boards BoardSet_boards_1_to_1(*)
                     test "u8001_u8001_dis" on boards BoardSet_boards_1_to_1(*)
                  end if
               end if
            end if
         end if
      end if
   end if
   call Update_Status (Status_Code, All_Failed)
subend

