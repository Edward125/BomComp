!new=====
sub InputMac
!call this sub to input Mac and Mac is in a global variable Get_Mac$
!(the other variable "SF_MAC$" used to save the MAC from SF)
global Get_MAC$
Get_MAC$=""
loop
 ! print "Please Scan MAC"

  input "Please Scan MAC:",Get_MAC$
   Get_MAC$=upc$(Get_MAC$)

! if Get_MAC$ = "" then Ident$ = Default_Serial_Number$
! exit if Ident$ = Default_Serial_Number$
  exit if  len(Get_MAC$) = 12
  print  "Please input right MAC , Length should be 12"
end loop
call CreateMacBin(Get_MAC$)

subend

sub CreateMacBin(Mac$)
!create a bin file from a given MAC,
!be sure you have the crc.exe in the current directory

dim Stemplate$[1000]
dim Bin$(16)[8]

Bin$(0)="0" & chr$(10) & "0" & chr$(10) & "0" & chr$(10) & "0"
Bin$(1)="0" & chr$(10) & "0" & chr$(10) & "0" & chr$(10) & "1"
Bin$(2)="0" & chr$(10) & "0" & chr$(10) & "1" & chr$(10) & "0"
Bin$(3)="0" & chr$(10) & "0" & chr$(10) & "1" & chr$(10) & "1"
Bin$(4)="0" & chr$(10) & "1" & chr$(10) & "0" & chr$(10) & "0"
Bin$(5)="0" & chr$(10) & "1" & chr$(10) & "0" & chr$(10) & "1"
Bin$(6)="0" & chr$(10) & "1" & chr$(10) & "1" & chr$(10) & "0"
Bin$(7)="0" & chr$(10) & "1" & chr$(10) & "1" & chr$(10) & "1"
Bin$(8)="1" & chr$(10) & "0" & chr$(10) & "0" & chr$(10) & "0"
Bin$(9)="1" & chr$(10) & "0" & chr$(10) & "0" & chr$(10) & "1"
Bin$(10)="1" & chr$(10) & "0" & chr$(10) & "1" & chr$(10) & "0"
Bin$(11)="1" & chr$(10) & "0" & chr$(10) & "1" & chr$(10) & "1"
Bin$(12)="1" & chr$(10) & "1" & chr$(10) & "0" & chr$(10) & "0"
Bin$(13)="1" & chr$(10) & "1" & chr$(10) & "0" & chr$(10) & "1"
Bin$(14)="1" & chr$(10) & "1" & chr$(10) & "1" & chr$(10) & "0"
Bin$(15)="1" & chr$(10) & "1" & chr$(10) & "1" & chr$(10) & "1"


Stemplate$ =           "2001000001d81028170c000035022000"
Stemplate$=Stemplate$ &"07f0180000000000ffffffffffffffff"
Stemplate$=Stemplate$ &"ffffffffffffffffffffffffffffffff"
Stemplate$=Stemplate$ &"ffffffffffffffffffffffffffffffff"
Stemplate$=Stemplate$ &"ffffffffffffffffffffffffffff0014"
Stemplate$=Stemplate$ &"22112233ffffffffffff0001ffffffff"
Stemplate$=Stemplate$ &"ffffffffffffffffffffffffffffffff"
Stemplate$=Stemplate$ &"ffffffffffffffffffffffffffff4610"

if len(Mac$)<>12 then
   printer is *
   print "Mac length must be  12 !!!"
   subexit
end if
Stemplate$[157;12] = Mac$

printer is "test.txt"
for I=1 to len(Stemplate$) step 4
   print Stemplate$[I;4]
next I
printer is *

unlink "crc.chk" ,Er
wait 10m
execute "crc.exe -t 0 -i test.txt -o crc.chk"
wait 100m

keyboard is "crc.chk"
input CRC$
keyboard is *
CRC$ =trimr$(CRC$)
CRC$ =triml$(CRC$)
if len(CRC$)=1 then CRC$ ="0" & CRC$


!print "CRC=" & CRC$
Stemplate$[253;2]=CRC$
printer is "crcread.txt"
print CRC$
printer is *

unlink "mac.bin"
wait 10m
printer is "Mac.bin"
for I=1 to len(Stemplate$)

    print Bin$(hti(Stemplate$[I;1]))
next I

printer is *

subend

sub Send_RRR
!For SF use ,Send out a require file
!With the format as they require
! global Serial_Length, Bad_Serial_Image$, Default_Serial_Number$
global Get_MAC$,MES_path$,BACKUP_path$,Oper$ ,Panel_Serial$

   MES_RRR$=MES_path$ &"/" &Panel_Serial$ &".RRR"
   BACKUP_RR1$=BACKUP_path$ &"/" &Panel_Serial$ &".RR1"
   BACKUP_RRR$=BACKUP_path$ &"/" &Panel_Serial$ &".RRR"

print   MES_RRR$
print   BACKUP_RR1$
print   BACKUP_RRR$

create ascii MES_RRR$, ,Er
 assign @MES,Er to MES_RRR$
if Er<>0 then print Er;"Failed out put to RRR file ,file name [" & MES_RRR$ & "]"

 output @MES;"[SFC]"
 output @MES;"USN=" & Panel_Serial$
 output @MES;"STAGE=TA"
 if Oper$="" then
     output @MES;"OPID=K0412008"
 else
     output @MES;"OPID=" &  Oper$
 end if
assign @MES to *
copy MES_RRR$ over BACKUP_RR1$ ,Er
rename  BACKUP_RR1$ to BACKUP_RRR$ ,Er
if Er<>0 then
   enter MES_RRR$,,Er
   if Er<>0 then
        printer is "AAAlog.txt";append
        print "read RRR failed file name [" & MES_RRR$ & "]"
        printer is *

   end if
end if

subend

sub Send_BBB(Board_I,ErrCode$,FixtureID$)
!Send out the test result file
!the format is as they required
! global Serial_Length, Bad_Serial_Image$, Default_Serial_Number$
global Get_MAC$,MES_path$,BACKUP_path$,Oper$, Panel_Serial$
global FaonOneBoard
   MES_BBB$=MES_path$ &"/" &Panel_Serial$ &".BBB"
   BACKUP_BB1$=BACKUP_path$ &"/" &Panel_Serial$ &".BB1"
   BACKUP_BBB$=BACKUP_path$ &"/" &Panel_Serial$ &".BBB"
create ascii MES_BBB$, ,Er

 assign @MES,Er to MES_BBB$

if Er<>0 then print Er;"Failed out put to BBB file ,file name [" & MES_BBB$ & "]"

 output @MES;"[SFC]"
 output @MES;"USN=" & Panel_Serial$
 output @MES;"STAGE=TA"
 output @MES;"ERRORCODE=" & ErrCode$
 output @MES;"TESTTIME=" & datetime$
 output @MES;"FIXTUREID=" & FixtureID$

 if Oper$="" then
     output @MES;"OPID=K0412008"
 else
     output @MES;"OPID=" &  Oper$
 end if

assign @MES to *

   if ErrCode$="0000" then          !!!!
       copy MES_BBB$ over BACKUP_BB1$   !!!!

       rename BACKUP_BB1$ to BACKUP_BBB$ !!!!
   else                             !!!!
   question"",Con                 !!!!
   if Con then                        !!!!
   !   copy MES_BBB$ over BACKUP_BB1$
    !  rename BACKUP_BB1$ to BACKUP_BBB$
   end if                                      !!!!
  !  copy MES_BBB$ over "z:\ate\fp6\" & Panel_Serial$ &".BBB" ,Errrord
   end if                                      !!!!
    call Send_EEE (Board_I)

! save retest time log
    create dir "TestLog"   ,ERRRRR

    Log_Log$=".\TestLog\log_" & datetime$[1;6]&".log"
    printer is Log_log$;append
    print Panel_Serial$ &"  " , FaonOneBoard ,"  " &  ErrCode$ & "  " & datetime$[1;12]
    printer is *
!end retest time log
!call Send_EEE(Board_I)

subend


sub Read_AAA
!read a answer file from SF
!get the data from SF,the main data is SF_MAC$,SF_ERRCODE$
global Get_MAC$,MES_path$,BACKUP_path$,Oper$,SF_MAC$,SF_ERRCODE$ ,  Panel_Serial$

   MES_AAA$=BACKUP_path$ &"/" &Panel_Serial$ &".AAA"

 Stime=msec
 print "Reading AAA..."
 loop
 assign @MES,Er to MES_AAA$

 exit if Er=0
 wait 10m

 if msec-Stime>6000 then
        print "Failed Open AAA file ,file name [" & MES_AAA$ & "]"

        fail device
        printer is "AAAlog.txt";append
        print "Failed Open AAA file ,file name [" & MES_AAA$ & "]"
        printer is *

        wait 1

        subexit
 end if
 end loop

 SF_MAC$=""
 SF_ERRCODE$=""

loop
enter @MES,,Err;Row$
   exit if Err = 101007
   exit if (SF_MAC$<>"" and SF_ERRCODE$<>"")
   if  pos(Row$,"MAC")=1 then SF_MAC$=trimr$(Row$[pos(Row$,"=")+1])
   if  pos(Row$,"ERRORCODE")=1 then SF_ERRCODE$=trimr$(Row$[pos(Row$,"=")+1])

end loop
assign @MES to *
! need burn LanID mac
!if len(SF_MAC$)<>12 then
!    fail device
!    report "Board not test ,MAC=" & SF_MAC$
!    subexit
!
!end if
!call CreateMacBin(SF_MAC$)

!if SF_MAC$<>Get_MAC$ then
!   print "Error : SFCS MAC is not equal Operator input Mac"
!   print "Error : SFCS MAC=" & SF_MAC$ & " ; Operater_input_Mac=" & Get_Mac$
!
!   fail device
!   report "Error : SFCS MAC=" & SF_MAC$ & " ; Operater_input_Mac=" & Get_Mac$
!   report "Board not test"
!   report out
!   report clear
!
!    printer is "AAAlog.txt";append
!    print  Serial$
!    print "Error : SFCS MAC=" & SF_MAC$ & " ; Operater_input_Mac=" & Get_Mac$
!    printer is *
!
!    copy MES_AAA$ over MES_AAA$ & ".er"
!
!    wait 2
!    subexit
!end if

if SF_ERRCODE$<>"0000" and SF_ERRCODE$<>"RTOK" then
   print "Error : SFCS show Error the the errcode:";SF_ERRCODE$
!  fail device
   report "Error : SFCS show Error the the errcode:";SF_ERRCODE$
!  report out
   report clear

    printer is "AAAlog.txt";append
    print  Panel_Serial$
    print "Error : SFCS show Error the the errcode:";SF_ERRCODE$
    printer is *

    wait 2
    subexit

end if
unlink MES_AAA$ ,Er

subend

sub Read_CCC

! global Serial_Length, Bad_Serial_Image$, Default_Serial_Number$
global Get_MAC$,MES_path$,BACKUP_path$,Oper$,SF_ERRCODE$  ,Panel_Serial$

   MES_CCC$=BACKUP_path$ &"/" &Panel_Serial$ &".CCC"

 Stime=msec
 print "Reading CCC..."

 loop
 assign @MES,Er to MES_CCC$
 exit if Er=0
 wait 10m
 if msec-Stime>2000 then
        print "Failed Open CCC file,file name [" & MES_CCC$ & "]"
        wait 1
        subexit
 end if
 end loop

 SF_ERRCODE$=""

loop
enter @MES,,Err;Row$
   exit if Err = 101007
   exit if ( SF_ERRCODE$<>"")
   if  pos(Row$,"ERRORCODE")=1 then SF_ERRCODE$=trimr$(Row$[pos(Row$,"=")+1])

end loop
assign @MES to *

unlink MES_CCC$ ,Er
SF_ERRCODE$=trimr$(SF_ERRCODE$)

if SF_ERRCODE$<>"0000" then
   print "SFCS save to DB Error ,Code=" & SF_ERRCODE$
   wait 2
end if

subend
sub Send_EEE (Board_I)
global Report_Printer$
global MES_path$,BACKUP_path$,Oper$
global Panel_Serial$,  Serial$(*)

board number is Board_I
MES_EEE$=MES_path$ & "\" &Panel_Serial$ & ".EEE"
BACKUP_EEE$=BACKUP_path$ & "\" &Panel_Serial$ & ".EEE"
BACKUP_EE1$=BACKUP_path$ & "\" &Panel_Serial$ & ".EE1"
if boardfailed then
   report is MES_EEE$
   report out
   report is Report_Printer$


!copy MES_EEE$ over "z:\ate\fp9\" & Panel_Serial$ & ".EEE" ,ERRRs

 copy MES_EEE$ over BACKUP_EE1$
 rename BACKUP_EE1$ to BACKUP_EEE$
end if

subend

sub Powerw
 global Panel_Serial$

 printer is "Powerlog";append

 randomize
 print "{@BLOCK|cp|01"
 V=1.05 + rnd*0.04
 Vs$=val$(V)
! print V,Vs$
! Vs$=substring$(Vs$,8)
 print "{@A-MEA|0|+" & Vs$ &"E+00|1.05V{@LIM2|+1.102500E+00|+9.975000E-01}}"
 V=1.8 + rnd*0.05
 Vs$=val$(V)
 print "{@A-MEA|0|+" & Vs$ &"E+00|1.8V{@LIM2|+1.890000E+00|+1.710000E+00}}"
 V=1.5 + rnd*0.06
 Vs$=val$(V)
 print "{@A-MEA|0|+" & Vs$ &"E+00|1.5V{@LIM2|+1.575000E+00|+1.425000E+00}}"
 V=3.3 + rnd*0.08
 Vs$=val$(V)
 print "{@A-MEA|0|+" & Vs$ &"E+00|3V{@LIM2|+3.465000E+00|+3.135000E+00}}"
 V=5 + rnd*0.15
 Vs$=val$(V)
 print "{@A-MEA|0|+" & Vs$ &"E+00|5V{@LIM2|+5.250000E+00|+4.750000E+00}}"
 print Panel_Serial$
 print "}"
 randomize
 printer is *
subend

sub Store_Log_Level
 global Stored_Log_Level$
 Stored_Log_Level$ = lli$
subend


sub Restore_Log_Level
 global Stored_Log_Level$

 if Stored_Log_Level$ = "all" then
   log level is all without nhls
 else
   if Stored_Log_Level$ = "failures" then
     log level is failures without nhls
   else
     if Stored_Log_Level$ = "none" then
       log level is none
     else
       if Stored_Log_Level$ = "analog" then
         log level is analog without nhls
       else
         if Stored_Log_Level$ = "board" then
           log level is board
         else
           if Stored_Log_level$ = "manual" then
             log level is manual
           end if
         end if
       end if
     end if
   end if
 end if

if learning then log level is all

subend


