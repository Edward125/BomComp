sub Analog_Functional_Tests (Status_Code, Message$)
global All_Failed
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_3_to_3(*)
global BoardSet_boards_2_to_2(*)

   if Message$ <> "" then  print tab(5);Message$
   call Panel_Enable_Boards
   test "hall1" on boards BoardSet_boards_1_to_1(*)
   test "pq4303%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq4502%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq4504%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq4601%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq4602%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq4901%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq8601%onoff" on boards BoardSet_boards_1_to_1(*)
   test "pq8602%onoff" on boards BoardSet_boards_1_to_1(*)
   test "q701%onoff" on boards BoardSet_boards_1_to_1(*)
