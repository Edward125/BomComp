Directory_Exists         = 100312
Log_Path$                = btgetenv$("AGILENT3070_ROOT")&"/qm/logdata/testerq/"
enter "uname -n |";System$              ! gets hostname (without domain)
Max_Times_To_Fix_Contact = 4            ! MUST BE >= 1
Pin_Msg$                 = "FIXTURE CONTACT PROBLEM"
