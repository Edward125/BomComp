   test "q3902%onoff" on boards BoardSet_boards_1_to_1(*)
   test "q3903%onoff" on boards BoardSet_boards_1_to_1(*)
   test "q4002%onoff" on boards BoardSet_boards_1_to_1(*)
   test "q4202%onoff" on boards BoardSet_boards_1_to_1(*)
